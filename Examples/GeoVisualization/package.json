{
  "name": "op-model-visualisation",
  "description": "Visualisation of an Operating Model with free flowing interaction",
  "version": "0.0.98",
  "license": "MIT",
  "dependencies": {
    "@geoman-io/leaflet-geoman-free": "^2.14.2",
    "d3": "^7.9.0",
    "leaflet": "^1.9.4",
    "leaflet-minimap": "^3.6.1"
  },
  "devDependencies": {
    "@babel/core": "^7.22.9",
    "@babel/preset-env": "^7.22.9",
    "@babel/preset-react": "^7.9.4",
    "babel-loader": "^8.1.0",
    "clean-webpack-plugin": "^4.0.0",
    "copy-webpack-plugin": "^11.0.0",
    "core-js": "^3.23.3",
    "ejs": "^3.1.10",
    "file-loader": "^6.0.0",
    "html-loader": "^1.1.0",
    "html-webpack-plugin": "^5.5.0",
    "jest": "^29.6.1",
    "jest-environment-jsdom": "^29.6.1",
    "json-loader": "^0.5.7",
    "standard": "^17.1.0",
    "uuid": "^8.0.0",
    "webpack": "^5.76.1",
    "webpack-cli": "^5.1.4",
    "webpack-dev-server": "^4.9.3",
    "webpack-merge": "^5.9.0",
    "webpack-version-file-plugin": "^0.5.0",
    "zip-webpack-plugin": "^4.0.1"
  },
  "scripts": {
    "generate-guids": "node generate-guids/generate-guids.js",
    "start": "webpack-dev-server  --config webpack.dev.js --open",
    "start01": "webpack-dev-server  --config webpack.dev.js --open --env viz=01",
    "start02": "webpack-dev-server  --config webpack.dev.js --open --env viz=02",
    "version": "webpack --config webpack.prod.js",
    "build": "npm version patch --no-git-tag-version",
    "rebuild": "webpack --config webpack.prod.js",
    "stats": "webpack --config webpack.prod.js --profile --json > stats.json",
    "check-style": "standard \"src/**/*.js\"",
    "fix-style": "standard \"src/**/*.js\" --verbose --fix",
    "lint": "eslint src",
    "test": "jest",
    "test-init": "jest --init"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "jsx"
    ],
    "moduleDirectories": [
      "node_modules"
    ],
    "moduleNameMapper": {
      "\\.(css|less)$": "<rootDir>/__mocks__/styleMock.js",
      "\\.(gif|ttf|eot|svg)$": "<rootDir>/__mocks__/fileMock.js"
    }
  },
  "standard": {
    "ignore": [
      "**/__tests__"
    ]
  }
}
